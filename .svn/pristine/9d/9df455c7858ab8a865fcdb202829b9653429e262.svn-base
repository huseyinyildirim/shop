body{position:relative;height:100%;font:12px tahoma,arial,verdana,sans-serif;background:#fff;/*color:#2C4B9C*/}
input,textarea,span,label{color:#525252}
*+html #top #sag #Label1_Container{margin-top:2px}
a{outline:none;text-decoration:underline}
a:hover,a:visited{text-decoration:none}
input,textarea,span,label{color:#525252, border:1px solid #CCC;}
table{border-collapse:collapse}
ul{list-style:none}
.default{ /*background:url(../images/bg.jpg) center top no-repeat;background-size:100% 100%;*/ background: url(../images/bg.jpg) no-repeat center bottom fixed; -webkit-background-size: cover;-moz-background-size: cover;-o-background-size: cover;background-size: cover}
#dtop{width:100%;height:40px;border-bottom:1px solid #67B3AC; background:#BFEDD1}
#dtopl{float:left;height:40px}
#dtopl img{margin-top:10px;margin-left:10px}
#dtopr{float:right;height:40px}
#dtopl h1{float:left;color:#528F89;font:18px Tahoma, Geneva, sans-serif;font-weight:bold;letter-spacing:-1px;line-height:40px;margin-left:20px;text-shadow: -1px 1px 1px rgba(121, 211, 202, 1)}
#dbottom{width:100%;height:32px;background:#010101 url(../images/bottom.jpg) top repeat-x;border-top:1px solid #547370}
#dbottoml{float:left;height:32px}
#dbottoml p{line-height:32px;margin-left:10px;color:#579892;font-size:11px}
#dbottomr{float:right;height:32px} 
#dbottomr p{}
#dbottomr p a{line-height:32px;margin-right:10px;color:#579892;font-size:11px} 
#noscript{background:red;border:3px solid #AD4B00;padding:5px;color:#fff;position:fixed;margin:14px 0 0 14px} 
span#LinkButton_1 img{float:left;vertical-align:middle;margin-top:6px}
span#LinkButton_1 a{display:block;font-weight:bold;color:#579892;text-decoration:none;height:32px;line-height:30px}
span#LinkButton_1 a:hover{}
.xtb-text{color:#579892}
.loading{min-width:380px;min-height:300px;background:url(../images/loading.gif) no-repeat center center}

/* desktop.aspx */
.left {float:left; display:inline;}
.right {float:right; display:inline;}